+<?php
+
+namespace App\Observers;
+
+use App\Models\User;
+use App\Services\ApiRequest\ApiRequestInterface;
+use App\Services\Yandex\YandexService;
+
+class UserSavedObserver
+{
+    private $request;
+
+    private $endPoint;
+
+    public function __construct(ApiRequestInterface $request)
+    {
+        $this->request = $request;
+        $this->endPoint = config('yandex_api_endpoint') . '/users';
+    }
+
+    public function saved(User $user)
+    {
+        if (!$user->import_id) {
+            return;
+        }
+
+        $token = $this->getCompanyToken($user->company->id);
+
+        if (!$token) {
+            return;
+        }
+
+        $syncService = new YandexService($this->request, $this->endPoint, $token);
+
+        $data = [
+            'name' => $user->name,
+            'department_id' => $user->department_id,
+            'nickname' => $user->login,
+            'position' => $user->position
+        ];
+
+        $response = $user->wasRecentlyCreated ? $syncService->store($data) : $syncService->update($user->import_id, $data);
+
+        dd($response);
+    }
+}
